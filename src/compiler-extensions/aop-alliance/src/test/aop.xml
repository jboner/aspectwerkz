<aspectwerkz>
    <system id="aopalliance">
       <aspect class="test.aopalliance.aspect.TestInterceptor">
            <advice name="invoke"
                type="around"
                bind-to="execution(long test.aopalliance.Test.execution()) || call(long test.aopalliance.Test.call())"/>
